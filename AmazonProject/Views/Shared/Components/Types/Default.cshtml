@{
}

@model IEnumerable<string>

<div class="card card-outline-primary m-1 p-1">
    <!--Bootstrap changes to make it look prettier-->
    <div class="bg-faded p-1">
        <h4>
            Title: @b.Title
            <span class="btn" style="float:right">
                <small>$@b.Price</small>
            </span>
        </h4>
        <p>
            <b>Author:</b> @b.Author
        </p>
    </div>
    <div class="card-text p-1"><b>Publisher: </b>@b.Publisher       <b>Classification: </b>@b.Classification</div>
    <div class="card-text p-1"><b>ISBN: </b>@b.Isbn <b>Page Count: </b>@b.PageCount</div>
</div>
